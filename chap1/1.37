(define (cont-frac n d k)
 (define (cont-frac-internal n d i k)
  (if (<= i k)
   (/ (n i) (+ (d i) (cont-frac-internal n d (+ i 1) k)))
   0)
 )
 (cont-frac-internal n d 1 k)
)

(define (cont-frac-iter n d k)
 (define (cont-frac-internal n d i result)
  (if (= i 0)
   result
   (cont-frac-internal n d (- i 1) (/ (n i) (+ (d i) result))))
 )
 (cont-frac-internal n d k 0)
)

(cont-frac (lambda (i) 1.0) (lambda (i) 1.0) 11)
(cont-frac-iter (lambda (i) 1.0) (lambda (i) 1.0) 11)
